{
    "name": "sashalenz/has-many-count",
    "description": "A Laravel Nova Count Relation field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/sashalenz/has-many-count",
    "authors": [
        {
            "name": "Aleksandr Petrovskyi",
            "email": "sashalenz@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sashalenz\\HasManyCount\\": "src/"
        }
    },
    "extra": {
